<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Set up Autowiring -->
	<bean id="userDAO" class="com.revature.dao.UserDAO"
		scope="prototype" autowire="byName"></bean>
	<bean id="userService"
		class="com.revature.service.UserServiceImpl" scope="prototype"
		autowire="byName"></bean>
	<bean id="sf" class="com.revature.util.SessionFactoryUtil" autowire="byName"></bean>
	
</beans>
